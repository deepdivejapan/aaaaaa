<p id="notice"><%= notice %></p>

<div class="event-box center center-block">
  <strong>Name:</strong>
  <%= @contact.name %>
<p>
  <strong>Email:</strong>
  <%= @contact.email %>
</p>
<p>
  <strong>Content:</strong>
  <%= @contact.content %>
